\\Criando banco de dados 
CREATE database Nome_Banco;

\\user do banco 
Use Nome_Banco;


\\Criando tabelas 
CREATE TABLE Minha_Tabela (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nome VARCHAR(100),
    idade INT,
    email VARCHAR(100),
    telefone VARCHAR(20),
    endereco VARCHAR(200)
);


\\Dados em tabela 
INSERT INTO Nome_da_Tabela (nome, idade, email, telefone, endereco) 
VALUES ('Pedro', '21','pedrohr@gmail.com', '11 96852543', 'Rua campanha gaucha');

\\Visualizar Tabela 

SELECT * FROM Nome_da_Tabela 
